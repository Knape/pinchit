{
  "name": "pinchit",
  "version": "0.5.0",
  "description": "Zoom images without dependencies",
  "keywords": [
    "pinch",
    "zoom",
    "mobile",
    "touch",
    "javascript",
    "vanilla",
    "es6"
  ],
  "main": "lib/index.js",
  "scripts": {
    "clean": "node_modules/.bin/rimraf ./lib",
    "build:lib": "node_modules/.bin/babel src/ -d lib/",
    "build": "npm run clean && npm run build:lib",
    "lint": "node_modules/.bin/eslint src/",
    "test": "node_modules/.bin/karma start test/karma.conf.js",
    "demo:watch": "webpack-dev-server --inline --config webpack.config.js --content-base docs --port 3050 --host 0.0.0.0",
    "demo:build": "NODE_ENV=production webpack",
    "prepublish": "npm run lint && npm run build"
  },
  "devDependencies": {
    "babel-cli": "^6.7.7",
    "babel-core": "^6.8.0",
    "babel-eslint": "^6.0.2",
    "babel-loader": "~6.2.4",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-3": "^6.17.0",
    "chai": "^3.2.0",
    "chai-spies": "^0.7.1",
    "css-loader": "^0.26.1",
    "eslint": "3.15.0",
    "eslint-config-airbnb-base": "11.1.0",
    "eslint-plugin-flowtype": "^2.30.0",
    "eslint-plugin-import": "2.2.0",
    "flow-bin": "^0.38.0",
    "istanbul": "~0.3.17",
    "istanbul-instrumenter-loader": "^0.2.0",
    "karma": "^0.13.22",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^0.5.5",
    "karma-firefox-launcher": "^1.0.0",
    "karma-fixture": "^0.2.6",
    "karma-html2js-preprocessor": "^1.1.0",
    "karma-json-fixtures-preprocessor": "0.0.6",
    "karma-mocha": "^0.2.2",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-sinon": "1.0.5",
    "karma-sinon-chai": "^1.2.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.0",
    "mocha": "^2.2.5",
    "prismjs": "^1.6.0",
    "sinon": "1.17.7",
    "sinon-chai": "^2.8.0",
    "style-loader": "^0.13.1",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.3.0"
  },
  "author": "Philip Knape",
  "license": "MIT"
}
